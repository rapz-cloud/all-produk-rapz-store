        </section>
        
        <section class="payment-section">
            <h2 class="payment-title"><i class="fas fa-qrcode"></i> QRIS</h2>
            <div class="qris-container">
                <img src="https://files.catbox.moe/uth34q.jpg" alt="QR Code" class="qris-image">
