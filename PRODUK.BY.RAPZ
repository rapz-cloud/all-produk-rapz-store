        </section>
        
        <footer>
            <p>Â© 2023 RAPZ STORE PAYMENT - All rights reserved</p>
        </footer>
    </div>

    <script>
        function copyToClipboard(text, method) {
            navigator.clipboard.writeText(text).then(function() {
                alert(`Nomor ${method} berhasil disalin: ${text}`);
            }, function() {
                alert('Gagal menyalin teks. Silakan salin manual.');
            });
        }
        
        // Efek petir acak
        function randomLightning() {
            const lightning = document.querySelector('.lightning');
            setTimeout(() => {
                lightning.style.left = Math.random() * 100 + '%';
                randomLightning();
            }, 3000);
        }
        
        randomLightning();
    </script>
</body>
</html>